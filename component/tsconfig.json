{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["ESNext", "DOM"],
        "jsx": "react",
        "moduleResolution": "Node",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": ["./src/**/*"]
}
